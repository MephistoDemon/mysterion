{
  "name": "mysterion",
  "version": "0.0.1",
  "author": "Andrej Novikov <jazzaiman@gmail.com>",
  "description": "Mysterion is desktop application to access Mysterion Network - decentralized VPN built on blockchain.",
  "license": "GPL-3.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "download:bins": "util_scripts/download-binaries.sh",
    "copy:bins": "util_scripts/copy-binaries.sh",
    "build": "node .electron-vue/build.js && electron-builder --publish never",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "unit": "karma start test/unit/karma.conf.js --single-run=true",
    "test:integration": "karma start test/integration/karma.conf.js --single-run=true",
    "test:integration:start-server": "karma start test/integration/karma.conf.js",
    "unit:start-server": "karma start test/unit/karma.conf.js",
    "unit:run": "karma run test/unit/karma.conf.js",
    "postinstall": "npm run && npm run lint:fix",
    "ci": "yarn lint && yarn flow && yarn unit"
  },
  "build": {
    "productName": "Mysterion",
    "appId": "network.mysterium.Mysterion",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      "./bin/openvpn",
      "./bin/mysterium_client",
      "./bin/openvpn",
      "./bin/config"
    ],
    "extraResources": [
      "./static/icons/trayActiveTemplate.png",
      "./static/icons/trayActiveTemplate@2x.png",
      "./static/icons/trayPassiveTemplate.png",
      "./static/icons/trayPassiveTemplate@2x.png",
      "./static/terms/terms.html"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/myst_logo.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons",
      "target": "AppImage",
      "category": "Network"
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "dbly-linked-list": "^0.2.0",
    "file-size": "^1.0.0",
    "jpex": "^2.1.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lolex": "^2.3.2",
    "md5": "^2.2.1",
    "raven": "^2.4.1",
    "raven-js": "^3.22.3",
    "sudo-prompt": "^8.1.0",
    "tail": "^1.2.3",
    "vue": "^2.5.13",
    "vue-electron": "^1.0.6",
    "vue-inject": "^2.0.1",
    "vue-multiselect": "^2.0.8",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.13",
    "axios-mock-adapter": "^1.14.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.3.1",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.2",
    "electron-builder": "^19.56.2",
    "electron-debug": "1.4.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.68.0",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "require-dir": "^0.3.0",
    "sass-loader": "^6.0.6",
    "spectron": "^3.8.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^14.2.2",
    "vue-style-loader": "^3.1.2",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.0"
  }
}
